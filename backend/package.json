  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "installGlobals": "npm i pm2 -g",
    "buildWin": "(if exist \".\\dist\\\" rd /s /q .\\dist\\)&&tsc",
    "build": "if [ ! -d \"$DIRECTORY\" ]; then rm -rf ./dist; fi ; tsc # not sure if it is operable",
    "startDevWin": "npm run buildWin && cross-env DEBUG=swagger-tools:middleware:* node --inspect ./dist/",
    "nodemon": "nodemon -w src/**/*.* -x \"npm run startDevWin\"",
    "startDevWinBrk": "npm run buildWin && node --inspect-brk ./dist/",
    "tsNodeDev": "cross-env DEBUG=swagger-tools:middleware:* ts-node-dev ./src/ --inspect --respawn",
    "tsNode": "ts-node ./src/ --inspect",
    "start": "cross-env NODE_ENV=\"production\" npm run buildWin && node ./dist/",
    "loadFixtures": "node ./fixtures/populate ./fixtures/database"
  },
